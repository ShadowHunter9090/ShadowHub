end)

btns:Seperator()

btns:Button("Icehub", function()
loadstring(game:HttpGet("https://scriptblox.com/raw/Universal-Script-Ice-Hub-6414"))()
DiscordLib:Notification("Bilgilendirme!", "IceHub Aktiflesdirildi", "Tamam!")
end)

btns:Seperator()

