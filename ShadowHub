local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()

local win = DiscordLib:Window("Shadow Hub V1.0")

local serv = win:Server("Brookhaven Hileleri", "http://www.roblox.com/asset/?id=6031075938")

