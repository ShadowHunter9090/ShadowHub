local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/ShadowHunter9090/ShadowHub/main/ShadowHubOrionLibraryCode')))()
local Window = OrionLib:MakeWindow({Name = "ShadowHub", HidePremium = false, SaveConfig = true, ConfigFolder = "ShadowHub"})


local Tab1 = Window:MakeTab({
	Name = "Ayarlar",
	Icon = "rbxassetid://7743866778",
	PremiumOnly = false
}) 

local Section = Tab1:AddSection({
	Name = "Ayarlar"
})


Tab1:AddToggle({
	Name = "Otur",
	Default = false,
	Callback = function(Value)
		if Value == true then
        game.Players.LocalPlayer.Character.Humanoid.Sit = true
        elseif Value == false then
        game.Players.LocalPlayer.Character.Humanoid.Sit = false
        end
})


local WalkSpeed

local JumpPower


Tab1:AddSlider({
	Name = "Hız",
	Min = 0,
	Max = 100,
	Default = 16,
	Color = Color3.fromRGB(125,125,125),
	Increment = 1,
	ValueName = "",
	Callback = function(Value)
    WalkSpeed = Value
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
         Value = WalkSpeed
	end    
})

Tab1:AddButton({
	Name = "Hızı Sıfırla",
	Callback = function()
        OrionLib:MakeNotification({
	Name = "               Hız",
	Content = "Hızını Başarılıyla Sıfırladık Şimdiki Hızın (16)",
	Image = "rbxassetid://7733715400",
	Time = 5
})
WalkSpeed = 16
    	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        end
})



Tab1:AddSlider({
	Name = "Zıplama Gücü",
	Min = 0,
	Max = 1000,
	Default = 50,
	Color = Color3.fromRGB(125,125,125),
	Increment = 1,
	ValueName = "",
	Callback = function(Value)
    JumpPower = Value
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
        Value = JumpPower
	end    
})

Tab1:AddButton({
	Name = "Zıplama Gücünü Sıfırla",
	Callback = function()
        OrionLib:MakeNotification({
	Name = "               Zıplama Gücü",
	Content = "Zıplama Gücün Başarılıyla Sıfırladık Şimdi Zıplama Gücün (50)",
	Image = "rbxassetid://7733715400",
	Time = 5
})
JumpPower = 50
    	game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
        end
})













