local Default_Disable = false;
Tab:AddToggle({
	Name = "SÄ±nÄ±rsÄ±z ZÄ±plama",
	Default = false,
	Callback = function(Value)
        if Default_Disable == false then
            Default_Disable = true;
            return;
        end
        ToggleInfJump(Value);
	end    
})






local WalkSpeed

local JumpPower


