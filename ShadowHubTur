local WalkSpeed

local JumpPower


Tab:AddToggle({
	Name = "Otur",
	Default = false,
	Callback = function(Value)
		if Value == true then
        game.Players.LocalPlayer.Character.Humanoid.Sit = true
        elseif Value == false then
        game.Players.LocalPlayer.Character.Humanoid.Sit = false
        end
})


